<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Modloader.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
    
    <Application.Styles>
        <FluentTheme />
    </Application.Styles>
    <Application.Resources>
        <Thickness x:Key="Thickness">2</Thickness>
        <Brush x:Key="Brush">#F01BCA</Brush>
        <CornerRadius x:Key="CornerRadius">10</CornerRadius>
        <Color x:Key="MainColor">#17181C</Color>
        <Color x:Key="SecondaryColor">#262930</Color>
        <Color x:Key="FontColor">#6C8AD5</Color>
    </Application.Resources>
</Application>